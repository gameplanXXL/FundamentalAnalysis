{
  "mcpServers": {
    "eodhd": {
      "command": "node",
      "args": ["dist/mcp-server.js"],
      "cwd": "/home/cneise/Project/fundamental",
      "env": {
        "EODHD_API_TOKEN": "${EODHD_API_TOKEN}"
      }
    },
    "firecrawl": {
      "command": "npx",
      "args": ["-y", "firecrawl-mcp"],
      "env": {
        "FIRECRAWL_API_KEY": "${FIRECRAWL_API_KEY}"
      }
    },
    "playwright": {
      "command": "npx",
      "args": ["@playwright/mcp@latest", "--headless"]
    }
  }
}
